@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix DE4LC: <http://research.tib.eu/DE4LC/vocab/> .
@prefix fnml: <http://semweb.mmlab.be/ns/fnml#> .
@prefix fno: <https://w3id.org/function/ontology#> .
@prefix biofunmap: <http://www.BioFunMap.com/> .
@prefix prov: <http://www.w3.org/ns/prov#> .

<Drug_drug_CUI>
    rml:logicalSource [ rml:source "/data/drug/CUI_DrugBank.csv";
                        rml:referenceFormulation ql:CSV 
                        ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/DE4LC/entity/{DrugBankID}";
        rr:class DE4LC:Drug
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [
            rr:parentTriplesMap <FalconDBpedia>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate owl:sameAs;
        rr:objectMap [
            rr:parentTriplesMap <FalconWikipedia>
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate DE4LC:hasCUIAnnotation;
        rr:objectMap [
            rr:parentTriplesMap <FalconUMLS>
        ]
    ].

<FalconDBpedia>
    rml:logicalSource [ rml:source "/data/drug/CUI_DrugBank.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap <FalconDBpedia-Function> ;
    rr:termType rr:IRI ;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "https://dbpedia.org/ontology/Drug";
        ]
    ]. 

<FalconWikipedia>
    rml:logicalSource [ rml:source "/data/drug/CUI_DrugBank.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap <FalconWikipedia-Function> ;
    rr:termType rr:IRI ;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "https://www.wikidata.org/wiki/Q11173";
        ]
    ].
    
<FalconUMLS>
    rml:logicalSource [ rml:source "/data/drug/CUI_DrugBank.csv";
                        rml:referenceFormulation ql:CSV
                        ];
    rr:subjectMap <UMLSFunction> ;
    rr:termType rr:IRI ;

    rr:predicateObjectMap [
        rr:predicate rdf:type;
        rr:objectMap [
            rr:template "http://research.tib.eu/DE4LC/vocab/Annotation";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate prov:hasGeneratedBy;
        rr:objectMap [
            rr:template "http://research.tib.eu/DE4LC/entity/UMLS"
        ]
    ].

##########################################################################################
######################################## Functions #######################################
##########################################################################################

<FalconDBpedia-Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/drug/CUI_DrugBank.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant biofunmap:falcon_dbpedia_function 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate biofunmap:value;
            rr:objectMap [ 
                rml:reference "DrugName" 
            ] 
        ];    
    ].

#####

<FalconWikipedia-Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/drug/CUI_DrugBank.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant biofunmap:falcon_wikipedia_function 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate biofunmap:value;
            rr:objectMap [ 
                rml:reference "DrugName" 
            ] 
        ];    
    ].           

#######

<UMLSFunction>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/drug/CUI_DrugBank.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant biofunmap:concat2 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate biofunmap:value1;
            rr:objectMap [ 
                rr:constant "http://research.tib.eu/DE4LC/Annotation/" 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate biofunmap:value2;
            rr:objectMap <FalconUMLS-Function>
        ];      
    ].

<FalconUMLS-Function>
    fnml:functionValue [
        rml:logicalSource [ rml:source "/data/drug/CUI_DrugBank.csv";
        rml:referenceFormulation ql:CSV 
                            ];
        rr:predicateObjectMap [
            rr:predicate fno:executes ;
            rr:objectMap [ 
                rr:constant biofunmap:falcon_UMLS_CUI_function 
            ]
        ];
        rr:predicateObjectMap [
            rr:predicate biofunmap:value;
            rr:objectMap [ 
                rml:reference "DrugName" 
            ] 
        ];    
    ].