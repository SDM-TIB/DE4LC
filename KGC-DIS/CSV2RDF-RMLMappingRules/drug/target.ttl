@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix DE4LC: <http://research.tib.eu/DE4LC/vocab/> .

<Target>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/drug/target.csv";
                rml:referenceFormulation ql:CSV
                ];
   rr:subjectMap [
		rr:template "http://research.tib.eu/DE4LC/entity/{Uni_ID}";
		rr:class DE4LC:Target,
			 DE4LC:Protein;
	];
	rr:predicateObjectMap [
		rr:predicate DE4LC:hasCUIAnnotation;
		rr:objectMap [
			rr:template "http://research.tib.eu/DE4LC/entity/{CUI}";
		]
	];	
	rr:predicateObjectMap [
		rr:predicate rdfs:label;
		rr:objectMap [
			rml:reference "Target_name";
		]
	];
	rr:predicateObjectMap [
		rr:predicate owl:sameAs;
		rr:objectMap [
			rr:template "http://purl.uniprot.org/uniprot/{Uni_ID}";
		]
	];
	rr:predicateObjectMap [
		rr:predicate DE4LC:hasSymbol;
		rr:objectMap [
			rr:template "http://research.tib.eu/DE4LC/entity/{Gene_name}";
		]
	].

