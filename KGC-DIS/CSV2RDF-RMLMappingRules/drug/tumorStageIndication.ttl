@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix DE4LC: <http://research.tib.eu/DE4LC/vocab/> .

<TumorStage_tumorStageIndication>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/drug/stages_drugs.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/DE4LC/entity/{stage}";
    ];
    rr:predicateObjectMap [
        rr:predicate DE4LC:tumorStage_has_indication;
        rr:objectMap [
        rr:template  "http://research.tib.eu/DE4LC/entity/{drugID}"
        ]
    ].

