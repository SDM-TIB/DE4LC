@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rev: <http://purl.org/stuff/rev#> .
@prefix schema: <http://schema.org/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix base: <http://tib.de/ontario/mapping#> .
@prefix DE4LC: <http://research.tib.eu/DE4LC/vocab/> .

<Publication_DE4LC_publication_author_LC>
    a rr:TriplesMap;
    rml:logicalSource [ rml:source "/data/pub/publication_LC_authors.csv";
                rml:referenceFormulation ql:CSV
                ];
    rr:subjectMap [
        rr:template "http://research.tib.eu/DE4LC/entity/{pmid}";
        rr:class DE4LC:Publication_Author
    ];
    rr:predicateObjectMap [
        rr:predicate dct:creator;
        rr:objectMap [
        rr:template  "http://research.tib.eu/DE4LC/entity/{author}"
        ]
    ].

